<UserControl x:Class="AutoFrontend.Wpf.Controls.StringControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d" MinHeight="50" MaxHeight="500">
    <GroupBox x:Name="groupBox" Header="ArgumentName">
       <TextBox TextWrapping="WrapWithOverflow" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" AcceptsTab="True"/>
    </GroupBox>
</UserControl>
